program = whitespace? statements? whitespace?
statements = statement (whitespace statement)*
statement = comment / literal / command
literal = string / number
command = identifier

string = ~'"([^\"]|\\")*"'
number = ~"-?\d+(\.\d+)?"

identifier = ~"[^\d\s\".]\S*"
whitespace = ~"\s+"
comment = ~"^#.*$"
